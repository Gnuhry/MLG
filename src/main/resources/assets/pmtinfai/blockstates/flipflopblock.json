{
  "multipart": [
    {
      "apply": {
        "model": "pmtinfai:block/flipflopblock_base"
      }
    },
    {
      "when": {
        "clock_side": "north"
      },
      "apply": {
        "model": "pmtinfai:block/logicblock_clock_input",
        "y": 180
      }
    },
    {
      "when": {
        "clock_side": "east"
      },
      "apply": {
        "model": "pmtinfai:block/logicblock_clock_input",
        "y": 270
      }
    },
    {
      "when": {
        "clock_side": "south"
      },
      "apply": {
        "model": "pmtinfai:block/logicblock_clock_input"
      }
    },
    {
      "when": {
        "clock_side": "west"
      },
      "apply": {
        "model": "pmtinfai:block/logicblock_clock_input",
        "y": 90
      }
    },
    {
      "when": {
        "set_side": "north"
      },
      "apply": {
        "model": "pmtinfai:block/logicblock_set_input",
        "y": 180
      }
    },
    {
      "when": {
        "set_side": "east"
      },
      "apply": {
        "model": "pmtinfai:block/logicblock_set_input",
        "y": 270
      }
    },
    {
      "when": {
        "set_side": "south"
      },
      "apply": {
        "model": "pmtinfai:block/logicblock_set_input"
      }
    },
    {
      "when": {
        "set_side": "west"
      },
      "apply": {
        "model": "pmtinfai:block/logicblock_set_input",
        "y": 90
      }
    },
    {
      "when": {
        "reset_side": "north"
      },
      "apply": {
        "model": "pmtinfai:block/logicblock_reset_input",
        "y": 180
      }
    },
    {
      "when": {
        "reset_side": "east"
      },
      "apply": {
        "model": "pmtinfai:block/logicblock_reset_input",
        "y": 270
      }
    },
    {
      "when": {
        "reset_side": "south"
      },
      "apply": {
        "model": "pmtinfai:block/logicblock_reset_input"
      }
    },
    {
      "when": {
        "reset_side": "west"
      },
      "apply": {
        "model": "pmtinfai:block/logicblock_reset_input",
        "y": 90
      }
    },
    {
      "when": {
        "output": "north"
      },
      "apply": {
        "model": "pmtinfai:block/logicblock_output",
        "y": 180
      }
    },
    {
      "when": {
        "output": "east"
      },
      "apply": {
        "model": "pmtinfai:block/logicblock_output",
        "y": 270
      }
    },
    {
      "when": {
        "output": "south"
      },
      "apply": {
        "model": "pmtinfai:block/logicblock_output"
      }
    },
    {
      "when": {
        "output": "west"
      },
      "apply": {
        "model": "pmtinfai:block/logicblock_output",
        "y": 90
      }
    },
    {
      "when": {
        "logic": "none"
      },
      "apply": {
        "model": "pmtinfai:block/flipflopblock_pc"
      }
    }
  ]
}