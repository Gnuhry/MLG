{
  "multipart": [
    {
      "apply": {
        "model": "pmtinfai:block/logicblock_base"
      }
    },
    {
      "when": {
        "OR": [
          {
            "clock_side": "north"
          },
          {
            "set_side": "north"
          },
          {
            "reset_side": "north"
          }
        ]
      },
      "apply": {
        "model": "pmtinfai:block/logicblock_input",
        "y": 180
      }
    },
    {
      "when": {
        "OR": [
          {
            "clock_side": "east"
          },
          {
            "set_side": "east"
          },
          {
            "reset_side": "east"
          }
        ]
      },
      "apply": {
        "model": "pmtinfai:block/logicblock_input",
        "y": 270
      }
    },
    {
      "when": {
        "OR": [
          {
            "clock_side": "south"
          },
          {
            "set_side": "south"
          },
          {
            "reset_side": "south"
          }
        ]
      },
      "apply": {
        "model": "pmtinfai:block/logicblock_input"
      }
    },
    {
      "when": {
        "OR": [
          {
            "clock_side": "west"
          },
          {
            "set_side": "west"
          },
          {
            "reset_side": "west"
          }
        ]
      },
      "apply": {
        "model": "pmtinfai:block/logicblock_input",
        "y": 90
      }
    },
    {
      "when": {
        "output": "north"
      },
      "apply": {
        "model": "pmtinfai:block/logicblock_output",
        "y": 180
      }
    },
    {
      "when": {
        "output": "east"
      },
      "apply": {
        "model": "pmtinfai:block/logicblock_output",
        "y": 270
      }
    },
    {
      "when": {
        "output": "south"
      },
      "apply": {
        "model": "pmtinfai:block/logicblock_output"
      }
    },
    {
      "when": {
        "output": "west"
      },
      "apply": {
        "model": "pmtinfai:block/logicblock_output",
        "y": 90
      }
    },
    {
      "when": {
        "logic": "none"
      },
      "apply": {
        "model": "pmtinfai:block/logicblock_pc",
        "y": 270
      }
    }
  ]
}